{
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2019"
        ],
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "strict": true,
        "module": "es2020",
        "target": "es2020",
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "noEmit": true
    }
}